#residence.section
  .row
    .col-xs-4.profile-section-image
      img.img-responsive(src=synapp.profile.residence.image)

    .col-xs-8
      h2 Residence

      .pre-text=synapp.profile.residence.description

  .residence-collapse.is-container
    .is-section

      .row
        .col-xs-6.text-muted.padding.text-center
          span.not-yet-validated
            i.fa.fa-exclamation-circle
            span  Not yet validated!
          span.hide.pre.is-validated
            i.fa.fa-check
            span  Validated 
            span.validated-moment date

        .col-xs-6
          button.btn.btn-block.validate-gps(disabled) GPS - Validate

  .row.toggle-arrow.padding.text-center
    i.fa.fa-arrow-down
