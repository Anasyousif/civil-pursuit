extends ../layout

block script-app
  - if ( settings.env === 'production' )
    script(src='/js/dashboard.min.js')

  - else
    script(src='/js/dashboard.js')

block page
  section.epics

    h1 Epics

    .epic.template-model
      h2.epic-name Epic Name
      .epic-description

      .epic-stories
        h3 Stories

        ul
          li.epic-story.template-model
            a(href='#').story-name Story
