{
  "name": "synapp",
  "version": "1.0.0",
  "description": "Synapp",
  "main": "index.js",
  "scripts": {
    "test": "babel app --out-dir dist && redtea dist/test/migrate dist/test/run --fork",
    "syntest": "node dist/bin/test",
    "start": "node dist/server/start.js",
    "magic-start": "npm install && npm start",
    "postinstall": "bash post-install.sh",
    "build": "bash bin/bundle.sh",
    "watchify": "bash bin/watchify.sh",
    "transpile": "babel app --ignore app/test --out-dir dist",
    "hot-transpile": "babel --watch app --ignore app/test --out-dir dist/",
    "less": "lessc assets/less/index.less assets/css/index.css && lessc assets/less/training.less assets/css/training.css",
    "watch-less": "npm run less && node dist/bin/watch-less",
    "min-css": "bash bin/min-css.sh",
    "uglify": "uglifyjs assets/js/main.js -o assets/js/main.min.js",
    "uglify-assets": "bash bin/uglify-assets.sh",
    "deploy": "bash bin/build.sh",
    "migrate": "node dist/bin/migrate",
    "reset": "node dist/bin/reset && node dist/bin/migrate",
    "clean": "node dist/bin/drop-sandbox-dbs.js",
    "packbuild": "webpack --config webpack-prod.config.js",
    "packstart": "npm run packbuild && node dist/server/start.js",
    "client": "webpack-dev-server --config webpack-dev.config.js --devtool inline-source-map",
    "server": "nodemon dist/server/start.js",
    "dev": "echo \"these delays need to be kept up to date\" && concurrently -k \"npm run hot-transpile\" \"npm run watch-less\" \"sleep 18 && npm run server\" \"sleep 24 && npm run client\"",
    "storybook": "start-storybook -s ./assets -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": {
    "email": "david@synaccord.com",
    "name": "David Fridley"
  },
  "license": "BSD-2-Clause",
  "engines": {
    "node": "~8.9.4",
    "npm": "6.1.0"
  },
  "dependencies": {
