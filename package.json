{
  "name": "synapp",
  "version": "1.0.0",
  "description": "Synapp",
  "main": "index.js",
  "scripts": {
    "test": "node dist/bin/drop-sandbox-dbs.js && redtea dist/test/migrate dist/test/run --fork",
    "syntest": "node dist/bin/test",
    "start": "node dist/bin/start.js",
    "magic-start": "npm install && npm start",
    "postinstall": "./post-install.sh",
    "build": "bash bin/bundle.sh",
    "watchify": "bash bin/watchify.sh",
    "transpile": "babel --out-dir dist/ app",
    "hot-transpile": "babel --watch --out-dir dist/ app",
    "less": "lessc assets/less/index.less assets/css/index.css; lessc assets/less/training.less assets/css/training.css",
    "watch-less": "npm run less && node dist/bin/watch-less",
    "min-css": "bash bin/min-css.sh",
    "uglify": "uglifyjs assets/js/main.js -o assets/js/main.min.js",
    "uglify-assets": "bash bin/uglify-assets.sh",
    "deploy-no-test": "bash bin/deploy-no-test.sh",
    "deploy": "bash bin/deploy.sh",
    "migrate": "node dist/bin/migrate",
    "reset" : "node dist/bin/reset && node dist/bin/migrate"
  },
  "author": {
    "email": "francoisrvespa@gmail.com",
    "name": "francois"
  },
  "license": "BSD-2-Clause",
  "engines": {
    "node": "5.x.x",
    "npm": "3.x.x"
  },
  "dependencies": {
    "autosize": "^3.0.15",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.12.2",
    "bower": "~1.3.12",
    "browserify": "~6.3.2",
    "cinco": "~0.1.3",
    "clean-css": "^3.2.9",
    "cloudinary": "~1.0.13",
    "colors": "~1.0.3",
    "cookie-parser": "~1.3.3",
    "express": "^4.12.2",
    "express-session": "~1.10.1",
    "firefox-profile": "^0.3.11",
    "less": "^2.5.0",
    "marked": "^0.3.3",
    "moment": "^2.10.6",
    "mungo": "^2.0.17",
    "nodemailer": "~1.3.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "passport-twitter": "^1.0.3",
    "promise-sequencer": "^2.0.1",
    "react": "~0.13.3",
    "socket.io": "^1.3.6",
    "socket.io-stream": "~0.6.0",
    "string": "^3.3.1",
    "superagent": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "redtea": "^3.4.1",
    "should": "^7.1.0",
    "socket.io-client": "^1.3.7",
    "uglify-js": "^2.4.21",
    "watchify": "~2.1.1",
    "webdriverio": "^3.4.0"
  }
}
