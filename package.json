{
  "name": "synapp",
  "version": "0.11.1",
  "description": "Synapp",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register test/replay/db.js test/replay/user.js test/replay/type.js test/replay/item.js test/replay/http.js test/replay/socket.js",
    "start": "node dist/bin/start.js",
    "magic-start": "npm install && npm start",
    "prepublish": "./post-install.sh",
    "build": "bash bin/bundle.sh",
    "watchify": "bash bin/watchify.sh",
    "transpile": "rm -rf dir && babel app/ --modules common --stage 1 --ignore app/lib/mung/node_modules --out-dir dist/",
    "hot-transpile": "rm -rf dir && babel app/ --watch --modules common --stage 1 --ignore app/lib/mung/node_modules --out-dir dist/",
    "less": "lessc assets/less/index.less assets/css/index.css; lessc assets/less/training.less assets/css/training.css",
    "watch-less": "npm run less && node dist/bin/watch-less",
    "min-css": "bash bin/min-css.sh",
    "uglify": "uglifyjs assets/js/main.js -o assets/js/main.min.js",
    "uglify-assets": "bash bin/uglify-assets.sh",
    "deploy-no-test": "bash bin/deploy-no-test.sh",
    "deploy": "bash bin/deploy.sh",
    "migrate": "node dist/bin/migrate"
  },
  "author": "francoisv <francoisrvespa@gmail.com>",
  "license": "BSD-2-Clause",
  "engines": {
    "node": "0.12.x"
  },
  "dependencies": {
    "async": "~0.9.0",
    "babel": "^5.4.7",
    "babel-runtime": "^5.8.25",
    "bcrypt": "~0.8.0",
    "bcryptjs": "^2.3.0",
    "body-parser": "^1.12.2",
    "bower": "~1.3.12",
    "browserify": "~6.3.2",
    "cinco": "~0.1.3",
    "clean-css": "^3.2.9",
    "cloudinary": "~1.0.13",
    "colors": "~1.0.3",
    "connect-multiparty": "~1.2.5",
    "cookie-parser": "~1.3.3",
    "express": "^4.12.2",
    "express-redis-cache": "^0.1.8",
    "express-session": "~1.10.1",
    "less": "^2.5.0",
    "marked": "^0.3.3",
    "mocha": "^2.3.3",
    "moment": "^2.10.6",
    "mung": "co2-git/mung",
    "nodemailer": "~1.3.0",
    "passport": "^0.2.2",
    "passport-facebook": "^2.0.0",
    "passport-twitter": "^1.0.3",
    "promise": "^7.0.1",
    "react": "~0.13.3",
    "request": "~2.48.0",
    "run-sequence": "~1.0.1",
    "should": "^7.1.0",
    "socket.io": "^1.3.6",
    "socket.io-client": "^1.3.7",
    "socket.io-redis": "~0.1.4",
    "socket.io-stream": "~0.6.0",
    "string": "^3.3.1",
    "superagent": "^1.3.0",
    "uglify-js": "^2.4.21",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "~2.1.1",
    "webdriverio": "^2.4.5"
  }
}
